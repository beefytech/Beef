				else if (next == 32)
					v = _val4;
				else
					v = 0;
				v >>= (start & 0x7) << 2;
				if (next > end)
					next = end;
				sb [--i] = (char8)('0' | (char8)(v & 0xf));
				switch (next - start) {
				case 8:
